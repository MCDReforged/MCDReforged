msgid "download"
msgstr "download"

#: ../cli/pim.rst:40
msgid "plugin_ids"
msgstr "plugin_ids"

#: ../cli/pim.rst:42
msgid "IDs of the plugins to be downloaded"
msgstr "将被下载的插件的 ID"

#: ../cli/pim.rst:44 ../cli/pim.rst:78
msgid "It can be supplied multiple times"
msgstr "可提供多次"

#: ../cli/pim.rst:47
msgid "-o, --output"
msgstr "-o, --output"

#: ../cli/pim.rst:49
msgid "Path of the directory to store the downloaded plugins"
msgstr "储存下载的插件的目标文件夹"

#: ../cli/pim.rst:51
