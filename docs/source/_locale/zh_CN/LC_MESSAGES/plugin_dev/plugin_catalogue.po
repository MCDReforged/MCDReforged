msgid "Example"
msgstr "例子"

#: ../plugin_dev/plugin_catalogue.rst:202
msgid "``<version>``"
msgstr "``<version>``"

#: ../plugin_dev/plugin_catalogue.rst:203
msgid "1.2.3"
msgstr "1.2.3"

#: ../plugin_dev/plugin_catalogue.rst:204
msgid "``v<version>``"
msgstr "``v<version>``"

#: ../plugin_dev/plugin_catalogue.rst:205
msgid "v1.2.3"
msgstr "v1.2.3"

#: ../plugin_dev/plugin_catalogue.rst:206
msgid "``<plugin_id>-<version>``"
msgstr "``<plugin_id>-<version>``"

#: ../plugin_dev/plugin_catalogue.rst:207
msgid "my_plugin_1.2.3"
msgstr "my_plugin_1.2.3"

#: ../plugin_dev/plugin_catalogue.rst:208
msgid "``<plugin_id>-v<version>``"
msgstr "``<plugin_id>-v<version>``"

#: ../plugin_dev/plugin_catalogue.rst:209
