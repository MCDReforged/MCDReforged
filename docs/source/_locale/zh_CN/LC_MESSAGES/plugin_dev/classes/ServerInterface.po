msgid "get_plugin_list"
msgstr "get_plugin_list"

#: ../../source/plugin_dev/classes/ServerInterface.rst:288
msgid ""
"Return a list containing all loaded plugin id like [\"my_plugin\", "
"\"another_plugin\"]"
msgstr ""

